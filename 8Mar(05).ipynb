    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Define the parameters of the binomial distribution\n",
    "n = 1\n",
    "p = 0.4\n",
    "\n",
    "# Generate the random sample of size 1000\n",
    "sample = np.random.binomial(n, p, size=1000)\n",
    "\n",
    "# Print the first 10 values of the sample\n",
    "print(sample[:10])\n",
    "plt.hist(sample)"
   ]
